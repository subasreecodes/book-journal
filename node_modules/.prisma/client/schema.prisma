generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Book {
  id          Int      @id @default(autoincrement())
  title       String
  author      String
  releaseYear Int
  myReview    String
  myRating    Float
  createdAt   DateTime @default(now())
}
